          function province() {
            $.ajax({
              type: "GET",
              url: "/province"
            }).done(function (data) {
              console.log('dta', data);
              let provinceOptions = data.map(function (item) {
                return `<option value="${item.province_id}">${item.province}</option>`
              })
              $('#provasal').html(provinceOptions.join(''));
              // $('#provinsi').html(provinceOptions.join(''));
            }).fail(function (err) {
              alert("error", err);
            });
          }
